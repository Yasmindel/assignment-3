import sys
sys.path.append('../')
from rosalind_utils import *

def edit():
    # Directly define the input data for this case
    recs = [("Rosalind_39", "PLEASANTLY"), ("Rosalind_11", "MEANLY")]
    
    seqa, seqb = recs[0][1], recs[1][1]  # "PLEASANTLY", "MEANLY"
    
    # Calculate the edit distance between the two sequences
    return edit_distance(seqa, seqb)

# Print the result of the edit distance
print(edit())
